{
  "kind": "collectionType",
  "collectionName": "user_details",
  "info": {
    "singularName": "user-detail",
    "pluralName": "user-details",
    "displayName": "user detail",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "phone": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "notifications": {
      "type": "component",
      "repeatable": false,
      "component": "user.app-notifications"
    },
    "contactPrivacy": {
      "type": "boolean",
      "default": false
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "municipality": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::municipality.municipality"
    }
  }
}
